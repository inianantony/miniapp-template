# Example Environment Variables
# Copy this file to .env.local to override defaults

# App Configuration
VITE_APP_NAME=myapp
VITE_BASE_PATH=/miniappsdev/myapp/
VITE_API_BASE=/miniappsdev/myapp/api
VITE_AUTH_TOKEN_URL=/miniappsdev/myapp/api/auth/token

# UserActivity API Configuration
# For development with mock API:
# VITE_USER_ACTIVITY_API_BASE=http://localhost:3001

# For testing with real API:
# VITE_USER_ACTIVITY_API_BASE=https://your-real-api.company.com

# For custom token (when using real API):
# VITE_CUSTOM_AUTH_TOKEN=your_bearer_token_here

# Debug
VITE_DEBUG=true